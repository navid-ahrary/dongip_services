{
  "name": "dongip_gateway",
  "version": "1.0.5",
  "description": "Dongip OpenAPI Explorer",
  "keywords": [
    "dongip",
    "loopback-application"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14.15.0"
  },
  "scripts": {
    "pull::production": "git pull origin main",
    "pull::development": "git pull origin development",
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t new .",
    "docker:run": "docker run -p 3000:3000 -d new",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "rebuild": "npm run clean && npm run build",
    "prestart": "npm run rebuild",
    "start::development": "node -r source-map-support/register .",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "restart": "pm2 restart ecosystem.config.js --env production",
    "reload": "pm2 reload ecosystem.config.js --env production --update-env",
    "stop": "pm2 stop ecosystem.config.js --env production",
    "prepublishOnly": "yarn run test",
    "deploy::production": "yarn pull::production && yarn && yarn build && yarn migrate && yarn reload",
    "deploy::development": "yarn pull::development && yarn && yarn clean && yarn build && yarn migrate && yarn reload"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navid-ahrary/dongip_services.git"
  },
  "author": "Dongip team",
  "contributors": [
    "Navid Ahrary"
  ],
  "license": "ISC",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^7.3.1",
    "@loopback/authentication-jwt": "^0.9.1",
    "@loopback/authorization": "^0.9.1",
    "@loopback/boot": "^3.4.1",
    "@loopback/cli": "^2.21.1",
    "@loopback/context": "^3.17.0",
    "@loopback/core": "^2.16.1",
    "@loopback/cron": "^0.5.1",
    "@loopback/express": "^3.3.1",
    "@loopback/extension-health": "^0.4.13",
    "@loopback/extension-metrics": "^0.3.13",
    "@loopback/openapi-v3": "^5.3.1",
    "@loopback/repository": "^3.7.0",
    "@loopback/rest": "^9.3.1",
    "@loopback/rest-explorer": "^3.3.1",
    "@loopback/service-proxy": "^3.2.1",
    "@woocommerce/woocommerce-rest-api": "^1.0.1",
    "awesome-phonenumber": "^2.48.0",
    "axios": "^0.21.1",
    "casbin": "^5.3.1",
    "countries-and-timezones": "^2.3.1",
    "debug": "^4.3.1",
    "deep-email-validator": "^0.1.18",
    "dotenv": "^8.2.0",
    "file-type": "^16.3.0",
    "firebase-admin": "^9.4.2",
    "helmet": "^4.4.1",
    "jalali-moment": "^3.3.10",
    "jsonwebtoken": "^8.5.1",
    "kavenegar": "^1.1.4",
    "lodash": "^4.17.21",
    "loopback-connector-mysql": "^6.0.0",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "qs": "^6.10.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@loopback/build": "^6.4.1",
    "@loopback/eslint-config": "^10.2.1",
    "@loopback/testlab": "^3.4.1",
    "@types/countries-and-timezones": "^2.0.3",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.149",
    "@types/morgan": "^1.9.0",
    "@types/multer": "^1.4.5",
    "@types/node": "^10.17.60",
    "@types/uuid": "^8.3.0",
    "@types/woocommerce__woocommerce-rest-api": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-plugin": "^3.0.3",
    "eslint-plugin-mocha": "^9.0.0",
    "source-map-support": "^0.5.19",
    "typescript": "~4.3.2"
  },
  "systemStatus": {
    "forceUpdate": false,
    "maintenance": false
  }
}
