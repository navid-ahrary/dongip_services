{
  "name": "dongip_gateway",
  "version": "1.0.5",
  "description": "Dongip OpenAPI Explorer",
  "keywords": [
    "dongip",
    "loopback-application"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=12.22.0"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t new .",
    "docker:run": "docker run -p 3000:3000 -d new",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "rebuild": "npm run clean && npm run build",
    "prestart": "npm ci && npm run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "prepublishOnly": "npm run test",
    "deploy": "npm prestart && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navid-ahrary/dongip_services.git"
  },
  "author": "Dongip team",
  "contributors": [
    "Navid Ahrary"
  ],
  "license": "ISC",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^9.0.2",
    "@loopback/authentication-jwt": "^0.12.2",
    "@loopback/authorization": "^0.12.2",
    "@loopback/boot": "^5.0.2",
    "@loopback/cli": "^4.1.2",
    "@loopback/context": "^5.0.2",
    "@loopback/core": "^4.0.2",
    "@loopback/cron": "^0.9.2",
    "@loopback/express": "^5.0.2",
    "@loopback/extension-health": "^0.4.13",
    "@loopback/extension-metrics": "^0.3.13",
    "@loopback/logging": "^0.9.2",
    "@loopback/openapi-v3": "^8.0.2",
    "@loopback/repository": "^5.0.2",
    "@loopback/rest": "^12.0.2",
    "@loopback/rest-explorer": "^5.0.2",
    "@loopback/service-proxy": "^5.0.2",
    "@woocommerce/woocommerce-rest-api": "^1.0.1",
    "awesome-phonenumber": "^3.3.0",
    "axios": "^0.27.2",
    "casbin": "^5.17.0",
    "countries-and-timezones": "^3.3.0",
    "debug": "^4.3.4",
    "deep-email-validator": "^0.1.21",
    "dotenv": "^16.0.1",
    "file-type": "18.0.0",
    "firebase-admin": "^11.0.1",
    "google-auth-library": "^8.4.0",
    "helmet": "^5.1.1",
    "jalali-moment": "^3.3.11",
    "jsonwebtoken": "^8.5.1",
    "kavenegar": "^1.1.4",
    "lodash": "^4.17.21",
    "loopback-connector-mysql": "^6.1.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.35",
    "multer": "^1.4.5-lts.1",
    "node-rsa": "^1.1.1",
    "qs": "^6.11.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@loopback/build": "^9.0.2",
    "@loopback/eslint-config": "^13.0.2",
    "@loopback/testlab": "^5.0.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.184",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.18.21",
    "@types/node-rsa": "^1.1.1",
    "@types/uuid": "^8.3.4",
    "@types/woocommerce__woocommerce-rest-api": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-plugin": "^4.4.1",
    "eslint-plugin-mocha": "^10.0.5",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.4.0",
    "typescript": "~4.7.4"
  },
  "systemStatus": {
    "forceUpdate": true,
    "maintenance": false
  }
}
