{
  "name": "dongip_services_gateway",
  "version": "0.9.5",
  "description": "Dongip OpenAPI Explorer",
  "keywords": [
    "dongip",
    "loopback-application"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=10.16.3"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "yarn run prettier:check && yarn run eslint",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -- -l",
    "prettier:fix": "yarn run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint -- --fix",
    "pretest": "yarn run clean && yarn run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "docker:build": "docker build -t dongip_service .",
    "docker :run": "docker run -p 8080:8080 -d dongip_services",
    "migrate": "node ./dist/migrate",
    "prestart": "yarn run build",
    "start": "nodemon -r source-map-support/register .",
    "deploy": "pm2 start ecosystem.config.js --env production",
    "stop": "pm2 stop ecosystem.config.js --env production",
    "prepublishOnly": "yarn run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/navid_ahrary/dongip_services.git"
  },
  "author": "Dongip team",
  "contributors": [
    "Navid Ahrary"
  ],
  "license": "ISC",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^5.0.0",
    "@loopback/authorization": "^0.6.4",
    "@loopback/boot": "^2.4.1",
    "@loopback/cli": "^2.12.0",
    "@loopback/context": "^3.9.4",
    "@loopback/core": "^2.9.3",
    "@loopback/cron": "^0.2.11",
    "@loopback/express": "^1.3.0",
    "@loopback/extension-health": "^0.4.11",
    "@loopback/extension-metrics": "^0.3.11",
    "@loopback/openapi-v3": "^3.4.7",
    "@loopback/repository": "^2.11.0",
    "@loopback/rest": "^6.0.0",
    "@loopback/rest-explorer": "^2.2.8",
    "@loopback/service-proxy": "^2.3.6",
    "awesome-phonenumber": "^2.27.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "casbin": "^5.0.4",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "firebase-admin": "^9.0.0",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "kavenegar": "^1.1.4",
    "lodash": "^4.17.19",
    "loopback-connector-mysql": "^5.4.3",
    "loopback-connector-rest": "^3.7.0",
    "moment": "^2.27.0",
    "morgan": "^1.10.0",
    "qs": "^6.9.4",
    "underscore": "^1.10.2",
    "zarinpal-checkout": "^0.2.7"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.0",
    "@loopback/eslint-config": "^9.0.0",
    "@loopback/testlab": "^3.2.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/helmet": "^0.0.47",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/lodash": "^4.14.149",
    "@types/morgan": "^1.9.0",
    "@types/node": "^14.0.27",
    "@types/underscore": "^1.9.4",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-mocha": "^8.0.0",
    "source-map-support": "^0.5.19",
    "typescript": "~3.9.7"
  }
}
