{
  "name": "dongip_gateway",
  "version": "1.0.5",
  "description": "Dongip OpenAPI Explorer",
  "keywords": [
    "dongip",
    "loopback-application"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=12.22.0"
  },
  "scripts": {
    "pull::production": "git pull origin main",
    "pull::development": "git pull origin development",
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run prettier:check && yarn run eslint",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -- -l",
    "prettier:fix": "yarn run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint -- --fix",
    "pretest": "yarn run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "docker:build": "docker build -t new .",
    "docker:run": "docker run -p 3000:3000 -d new",
    "premigrate": "yarn run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "yarn run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "rebuild": "yarn run clean && yarn run build",
    "prestart": "yarn run rebuild",
    "start::development": "node -r source-map-support/register .",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "restart": "pm2 restart ecosystem.config.js --env production",
    "reload": "pm2 reload ecosystem.config.js --env production --update-env",
    "stop": "pm2 stop ecosystem.config.js --env production",
    "prepublishOnly": "yarn run test",
    "deploy::production": "yarn pull::production && yarn && yarn build && yarn migrate && yarn reload",
    "deploy::development": "yarn pull::development && yarn && yarn clean && yarn build && yarn migrate && yarn reload"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navid-ahrary/dongip_services.git"
  },
  "author": "Dongip team",
  "contributors": [
    "Navid Ahrary"
  ],
  "license": "ISC",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^8.1.2",
    "@loopback/authentication-jwt": "^0.11.2",
    "@loopback/authorization": "^0.11.2",
    "@loopback/boot": "^4.1.2",
    "@loopback/cli": "^3.2.0",
    "@loopback/context": "^4.1.2",
    "@loopback/core": "^3.1.2",
    "@loopback/cron": "^0.8.2",
    "@loopback/express": "^4.1.2",
    "@loopback/extension-health": "^0.4.13",
    "@loopback/extension-metrics": "^0.3.13",
    "@loopback/logging": "^0.8.2",
    "@loopback/openapi-v3": "^7.1.2",
    "@loopback/repository": "^4.1.2",
    "@loopback/rest": "^11.1.2",
    "@loopback/rest-explorer": "^4.1.2",
    "@loopback/service-proxy": "^4.1.2",
    "@woocommerce/woocommerce-rest-api": "^1.0.1",
    "awesome-phonenumber": "^2.71.0",
    "axios": "^0.26.1",
    "casbin": "^5.15.0",
    "countries-and-timezones": "^3.3.0",
    "debug": "^4.3.4",
    "deep-email-validator": "^0.1.21",
    "dotenv": "^16.0.0",
    "file-type": "16.5.3",
    "firebase-admin": "^10.0.2",
    "helmet": "^5.0.2",
    "jalali-moment": "^3.3.11",
    "jsonwebtoken": "^8.5.1",
    "kavenegar": "^1.1.4",
    "lodash": "^4.17.21",
    "loopback-connector-mysql": "^6.1.0",
    "moment": "^2.29.3",
    "moment-timezone": "^0.5.34",
    "multer": "^1.4.4",
    "qs": "^6.10.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@loopback/build": "^8.1.2",
    "@loopback/eslint-config": "^12.0.4",
    "@loopback/testlab": "^4.1.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/multer": "^1.4.7",
    "@types/node": "^12.20.47",
    "@types/uuid": "^8.3.4",
    "@types/woocommerce__woocommerce-rest-api": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-eslint-plugin": "^4.1.0",
    "eslint-plugin-mocha": "^10.0.3",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.4.0",
    "typescript": "~4.6.3"
  },
  "systemStatus": {
    "forceUpdate": true,
    "maintenance": false
  }
}
