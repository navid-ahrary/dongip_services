{
  "name": "dongip",
  "version": "0.4.0",
  "description": "spilitin between friends",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=10.16.3"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "yarn run prettier:check && npm run eslint",
    "lint:fix": "yarn run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -- -l",
    "prettier:fix": "yarn run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint -- --fix",
    "pretest": "yarn run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t login_service .",
    "docker:run": "docker run -p 3000:3000 -d login_service",
    "migrate": "node ./dist/migrate",
    "prestart": "yarn run build",
    "start": "node -r source-map-support/register .",
    "prepublishOnly": "yarn run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/navid_ahrary/login_service.git"
  },
  "author": "navidarry",
  "license": "ISC",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^3.2.0",
    "@loopback/boot": "^1.5.9",
    "@loopback/cli": "1.23.2",
    "@loopback/context": "^1.23.3",
    "@loopback/core": "^1.10.5",
    "@loopback/openapi-v3": "^1.9.10",
    "@loopback/repository": "^1.15.2",
    "@loopback/rest": "^1.21.0",
    "@loopback/rest-explorer": "^1.4.2",
    "@loopback/service-proxy": "^1.3.9",
    "@types/uuid": "^3.4.5",
    "awesome-phonenumber": "^2.21.0",
    "bcryptjs": "^2.4.3",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "loopback-connector": "^4.5.0",
    "loopback-connector-kv-redis": "^3.0.2",
    "loopback-connector-mongodb": "^4.2.0",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@loopback/build": "^2.0.14",
    "@loopback/eslint-config": "^4.1.2",
    "@loopback/testlab": "^1.9.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lodash": "^4.14.146",
    "@types/node": "^10.14.20",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-eslint-plugin": "^2.1.0",
    "eslint-plugin-mocha": "^6.1.1",
    "source-map-support": "^0.5.13",
    "typescript": "~3.6.3"
  }
}
